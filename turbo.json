{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".env.local"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "pages:build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "pages:preview": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false
    },
    "db:drop": {
      "cache": false
    },
    "db:migration:local": {
      "cache": false
    },
    "db:migration:cloud": {
      "cache": false
    },
    "db:studio": {
      "cache": false
    },
    "add:comp": {
      "cache": false
    }
  }
}
